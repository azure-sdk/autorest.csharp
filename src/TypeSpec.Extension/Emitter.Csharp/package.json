{
  "description": "The typespec library that can be used to generate C# models from a TypeSpec REST protocol binding",
  "bugs": {
    "url": "https://github.com/Azure/autorest.csharp/issues"
  },
  "dependencies": {
    "winston": "^3.8.2",
    "@typespec/library-linter": "0.50.0-dev.0",
    "@typespec/rest": "0.50.0-dev.0",
    "@typespec/versioning": "0.50.0-dev.1",
    "@typespec/eslint-plugin": "0.49.0",
    "@typespec/compiler": "0.50.0-dev.4",
    "@azure-tools/typespec-azure-core": "0.36.0-dev.1",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "@typespec/http": "0.50.0-dev.0",
    "json-serialize-refs": "0.1.0-0",
    "@typespec/eslint-config-typespec": "0.50.0-dev.0",
    "@azure-tools/typespec-client-generator-core": "0.36.0-dev.3",
    "@autorest/csharp": "3.0.0-alpha.20231018.7.x"
  },
  "keywords": [
    "typespec"
  ],
  "author": "Microsoft Corporation",
  "version": "0.2.0-alpha.20231018.7.x",
  "name": "@azure-tools/typespec-csharp",
  "scripts": {
    "build": "tsc -p .",
    "prettier:fix": "prettier --write --config .prettierrc.json ./src/**/*.ts ./test/**/*.ts",
    "test-official": "c8 mocha --forbid-only dist/test/**/*.js",
    "watch": "tsc -p . --watch",
    "lint:fix": "eslint . --fix --ext .ts",
    "lint-typespec-library": "tsp compile . --warn-as-error --import @typespec/library-linter --no-emit",
    "test": "mocha dist/test/**/*.js",
    "clean": "rimraf ./dist ./temp",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "prettier": "prettier --check --config .prettierrc.json ./src/**/*.ts ./test/**/*.ts",
    "pack": "npx webpack"
  },
  "devDependencies": {
    "c8": "~7.11.0",
    "typescript": "~5.1.5",
    "lodash.isequal": "^4.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "~18.13.0",
    "@types/fs-extra": "^9.0.13",
    "@types/prettier": "^2.6.3",
    "mocha": "~9.2.0",
    "@types/mocha": "~9.1.0",
    "eslint": "^8.12.0",
    "rimraf": "~3.0.2",
    "@types/lodash.isequal": "^4.5.6"
  },
  "type": "module",
  "readme": "https://github.com/Azure/autorest.csharp/blob/feature/v3/src/TypeSpec.Extension/Emitter.Csharp/readme.md",
  "license": "MIT",
  "files": [
    "dist/src/**"
  ],
  "homepage": "https://github.com/Microsoft/typespec",
  "main": "dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.csharp.git"
  }
}